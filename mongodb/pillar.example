mongodb:
  enabled: True
  version: 4.0
  pymongo_version: == 3.12.3
  admin:
    authdb: admin
    user: admin
    password: xxxxxxxxxxxxxxxxx
    host: localhost
    port: 27017
    roles:
      - dbOwner
      - userAdmin
      - userAdminAnyDatabase
  databases:
    - name: admin
      users:
        - name: root
          password: xxxxxxxxxxxxxxxxx
          roles:
            - root
    - name: newDb
      users:
        - name: newUser
          password: xxxxxxxxxxxxxxxxx
          roles:
            - readWrite
            - dbOwner
